{
  "name": "skaarup.dev-v2",
  "version": "0.0.2",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "bun run ./build",
    "dev": "bunx --bun vite dev",
    "build": "bunx --bun vite build",
    "preview": "bunx --bun vite preview",
    "build:preview": "bunx --bun vite build && PORT=5173 bun ./build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "biome lint .",
    "lint:fix": "biome lint --apply .",
    "format": "biome format --write .",
    "fmt": "biome format --write ."
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@sveltejs/kit": "^2.5.24",
    "@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
    "@tailwindcss/vite": "^4.0.0-alpha.20",
    "@types/bun": "latest",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.43.0",
    "nanoid": "^5.0.7",
    "prettier": "^4.0.0-alpha.9",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "svelte": "^5.0.0-next.238",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^3.8.6",
    "sveltekit-plugin-console": "^0.1.11",
    "tailwindcss": "^4.0.0-alpha.20",
    "typescript": "^5.5.4",
    "vite": "^6.0.0-alpha.20"
  },
  "trustedDependencies": ["@biomejs/biome", "@sveltejs/kit", "esbuild", "svelte-preprocess"]
}
