{
  "name": "skaarup.dev-v2",
  "version": "0.0.2",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "bun run ./build",
    "dev": "bunx --bun vite dev",
    "build": "bunx --bun vite build",
    "preview": "bunx --bun vite preview",
    "build:preview": "bunx --bun vite build && PORT=5173 bun ./build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "biome lint .",
    "lint:fix": "biome lint --apply .",
    "format": "biome format --write .",
    "fmt": "biome format --write ."
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.0",
    "@sveltejs/kit": "^2.5.6",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@tailwindcss/vite": "^4.0.0-alpha.14",
    "@types/bun": "latest",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.37.0",
    "nanoid": "^5.0.7",
    "prettier": "^4.0.0-alpha.8",
    "prettier-plugin-svelte": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "svelte": "^5.0.0-next.107",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^3.6.9",
    "tailwindcss": "^4.0.0-alpha.14",
    "typescript": "^5.4.5",
    "vite": "^5.2.9"
  },
  "trustedDependencies": ["@biomejs/biome", "@sveltejs/kit", "esbuild", "svelte-preprocess"]
}
